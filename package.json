  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "analysis": "cross-env NODE_ENV=development MODE=analyse npm start",
    "dll:prepare": "npm run dll:prepare:dev & npm run dll:prepare:prd",
    "dll:prepare:dev": "cross-env NODE_ENV=development gulp dll:prepare",
    "dll:prepare:prd": "cross-env NODE_ENV=production gulp dll:prepare",
    "release": "cross-env NODE_ENV=production gulp release",
    "sprite:all": "gulp build.sprite --target all",
    "sprite:normal": "gulp build.sprite --target sprites",
    "sprite:mmc": "gulp build.sprite --target spritesMMC",
    "sprite:bc": "gulp build.sprite --target spritesBC",
    "sprite:pt": "gulp build.sprite --target spritesPT",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "docs": "cd docs && gitbook install && gitbook serve",
    "docs:deploy": "cd docs && ./deploy.sh",
    "e2e": "cross-env NODE_ENV=development node test/e2e/runner.js",
    "e2e-dev": "cross-env NODE_ENV=development node test/e2e/runner.js --dev",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit && npm run e2e-dev"
  },
